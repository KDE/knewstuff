#, kde-format
msgid "Configuration file does not exist: \"%1\""
msgstr "קובץ הגדרות לא קיים: „%1”"

#: core/enginebase.cpp:87
#, kde-format
msgid "Configuration file exists, but cannot be opened: \"%1\""
msgstr "קובץ תצורה קיים אך לא ניתן לפתיחה: „%1”"

